{
  "name": "typify",
  "description": "Runtime type-checking for JavaScript.",
  "version": "0.2.5",
  "homepage": "https://github.com/phadej/typify",
  "author": {
    "name": "Oleg Grenrus",
    "email": "oleg.grenrus@iki.fi",
    "url": "http://oleg.fi"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/phadej/typify.git"
  },
  "bugs": {
    "url": "https://github.com/phadej/typify/issues"
  },
  "licenses": [
    {
      "type": "BSD",
      "url": "https://github.com/phadej/typify/blob/master/LICENSE"
    }
  ],
  "main": "lib/typify",
  "engines": {
    "node": ">= 0.8.0"
  },
  "scripts": {
    "browserify": "mkdir -p dist/ && node_modules/.bin/browserify -s jsc -o dist/typify.standalone.js ./lib/typify.js",
    "jshint": "node_modules/.bin/jshint lib test",
    "mocha": "node_modules/.bin/mocha --reporter spec test",
    "istanbul": "node_modules/.bin/istanbul cover node_modules/.bin/_mocha test",
    "check-coverage": "node_modules/.bin/istanbul check-coverage --statements -11 --branches -5 --functions -6 coverage/coverage.json",
    "typify": "node_modules/.bin/typify -- node_modules/.bin/_mocha -c test",
    "test": "npm run-script jshint && npm run-script mocha && npm run-script istanbul && npm run-script check-coverage && npm run-script typify"
  },
  "devDependencies": {
    "jsverify": "~0.3.0",
    "istanbul": "~0.2.3",
    "mocha": "~1.17.0",
    "browserify": "~3.20.0",
    "jshint": "~2.4.1",
    "lodash": "~2.4.1",
    "typify": "phadej/typify-bin"
  },
  "keywords": [
    "type",
    "checking"
  ]
}
